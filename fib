def fibonacci(n):
    if n <= 0:
        return []
    elif n == 1:
        return [0]
    elif n == 2:
        return [0, 1]

    fib_sequence = [0, 1]
    for i in range(2, n):
        fib_sequence.append(fib_sequence[-1] + fib_sequence[-2])

    return fib_sequence

n = 20  # Generate the first 10 elements of the Fibonacci sequence
fib_sequence = fibonacci(n)
print(fib_sequence)
